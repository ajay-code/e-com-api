{
  "name": "ts-express-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/bin/server.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/bin/server.js\"",
    "build": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.4",
    "module-alias": "^2.2.2",
    "mongoose": "^5.13.7",
    "morgan": "~1.9.1",
    "tsc-watch": "^4.5.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.1"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@middleware": "dist/middleware",
    "@errors": "dist/errors",
    "@routes": "dist/routes",
    "@models": "dist/models",
    "@config": "dist/config",
    "@db": "dist/db"
  }
}
